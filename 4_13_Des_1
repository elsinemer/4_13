class Libro:
    def __init__(self, titulo, autor, isbn):
        self._titulo = titulo
        self._autor = autor
        self._isbn = isbn

    def get_titulo(self):
        return self._titulo

    def set_titulo(self, nuevo_titulo):
        self._titulo = nuevo_titulo

    def get_autor(self):
        return self._autor

    def set_autor(self, nuevo_autor):
        self._autor = nuevo_autor

    def get_isbn(self):
        return self._isbn

    def set_isbn(self, nuevo_isbn):
        self._isbn = nuevo_isbn
